{
    "name": "@sandbox/rollup",
    "version": "1.0.0",
    "private": "true",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "pnpm run clean && tsc --outDir dist --project tsconfig.json && mv ./dist/config/rollup/src/* ./dist && rm -rf ./dist/config"
    },
    "dependencies": {
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "rollup": "^4.9.0",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-dts": "^6.1.0",
        "rollup-plugin-esbuild": "^6.1.0"
    }
}
